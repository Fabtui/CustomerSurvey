<table class="table">
  <thead>
    <tr>
      <th scope="col"></th>
      <th><%= image_tag 'dislike.png', height: '25', width: '25'  %></th>
      <th><%= image_tag 'like.png', height: '25', width: '25'  %></th>
      <th>Total</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">Nombres</th>
      <td><%= day.bad %></td>
      <td><%= day.good %></td>
      <td><%= day.total %></td>
    </tr>
    <tr>
      <th scope="row">Poucentages</th>
      <td><%= ((day.bad * 100) / day.total).round %>%</td>
      <td><%= ((day.good * 100) / day.total).round %>%</td>
      <td>100%</td>
    </tr>
  </tbody>
</table>
